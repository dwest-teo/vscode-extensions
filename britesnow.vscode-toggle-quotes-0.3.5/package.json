{
	"name": "vscode-toggle-quotes",
	"displayName": "Toggle Quotes",
	"description": "Toggle cycle \" -> ' -> `",
	"version": "0.3.5",
	"publisher": "BriteSnow",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/BriteSnow/vscode-toggle-quotes"
	},
	"engines": {
		"vscode": "^1.17.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:editor.togglequotes"
	],
	"extensionKind": [
		"ui",
		"workspace"
	],
	"main": "./out/src/extension",
	"icon": "icon.png",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "toggle-quotes configuration",
			"properties": {
				"togglequotes.chars": {
					"type": "array",
					"default": [
						"\"",
						"'"
					],
					"description": "Type of quotes to toggle from."
				}
			}
		},
		"configurationDefaults": {
			"[typescript]": {
				"togglequotes.chars": [
					"\"",
					"'",
					"`"
				]
			},
			"[javascript]": {
				"togglequotes.chars": [
					"\"",
					"'",
					"`"
				]
			},
			"[typescriptreact]": {
				"togglequotes.chars": [
					"\"",
					"'",
					"`"
				]
			},
			"[javascriptreact]": {
				"togglequotes.chars": [
					"\"",
					"'",
					"`"
				]
			},
			"[svelte]": {
				"togglequotes.chars": [
					"\"",
					"'",
					"`"
				]
			},
			"[markdown]": {
				"togglequotes.chars": [
					"\"",
					"'",
					"`"
				]
			},
			"[go]": {
				"togglequotes.chars": [
					"\"",
					"'",
					"`"
				]
			}
		},
		"commands": [
			{
				"command": "editor.togglequotes",
				"title": "Toggle Quote"
			}
		],
		"keybindings": [
			{
				"command": "editor.togglequotes",
				"key": "ctrl+'",
				"mac": "cmd+'"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.7.1",
		"mocha": "^9.1.0",
		"typescript": "^4.3.5",
		"vscode": "^1.1.17"
	},
	"__metadata": {
		"id": "8d088af2-ecd1-4b8f-93e9-322125c47b3c",
		"publisherId": "4f0b999b-00a3-4e77-b16b-57e094b1ca59",
		"publisherDisplayName": "BriteSnow",
		"installedTimestamp": 1629766570095
	}
}